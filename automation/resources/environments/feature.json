{
    "feature_name" : "*{feature_name}-{layer_name}",
    "capacity_name": "MyCapacity",
    "git_settings": {
        "gitProviderDetails": {
            "gitProviderType": "GitHub",
            "ownerName": "MyGitHubProfile",
            "repositoryName": "MyGitHubRepo"
        },
        "myGitCredentials": {
            "source": "ConfiguredConnection",
            "connection_name": "MyGitConnectionName"
        }
    },
    "permissions": {
        "admin": [
            {"type": "Group", "id": "32a268ea-bfef-4637-962f-c6b266a12320"}
        ],
        "contributor": [
            {"type": "User", "id": "d55902b5-7fd6-46a0-b3dc-e7c7a33b8300"}
        ]
    },
    "layers" : {
        "Prepare": { 
            "spark_settings": {
                "pool": {
                    "starterPool":{
                        "maxExecutors": 1,
                        "maxNodeCount": 1
                    }
                }
            },
            "git_directoryName": "solution/prepare",
            "git_synchronize_on_commit": true
        },
        "Ingest": { 
            "git_directoryName": "solution/ingest",
            "git_synchronize_on_commit": false
        },
        "Orchestrate": {
            "git_directoryName": "solution/orchestrate",
            "git_synchronize_on_commit": false
        }
    }
}
